package org.Sujan;

import java.io.File;
import java.io.IOException;

public class App {

	public static void main(String[] args) {
		
		{
			File file = new File("text");
			file.mkdir();
			file = new File("text\\team.txt");
			try {
				file.createNewFile();
				System.out.println("File Created");
			} catch (IOException e) {
				System.out.println("error occured, while creating the file");
			}
		}
		

	}

}
